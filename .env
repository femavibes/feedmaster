# Database Connection
# For PostgreSQL using Docker Compose, the hostname is the service name (e.g., 'db')
SQLALCHEMY_DATABASE_URL="postgresql://user:password@db:5432/feedmaster_db"

# Bluesky API for DID resolution
BLUESKY_API_BASE_URL="https://api.bsky.app/xrpc"

# Secret key for JWT token generation (replace with a strong, random string!)
SECRET_KEY="your-super-secret-key-replace-me"

# Algorithm for JWT (HS256 is common)
ALGORITHM="HS256"

# Token expiration time in minutes
ACCESS_TOKEN_EXPIRE_MINUTES=30
